{
  "name": "simul",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "webserve": "vue-cli-service serve",
    "webbuild": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service electron:build && pyinstaller --noconfirm --add-data pysrc/db.sqlite;. --onefile pysrc/simulserver.py && cp dist/simulserver.exe dist_electron/win-unpacked/_simulserver.exe",
    "serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "codemirror": "^5.59.1",
    "core-js": "^3.6.5",
    "electron-fetch": "^1.7.3",
    "lodash": "^4.17.20",
    "metro4": "^4.4.3",
    "plotly.js-dist": "^1.58.4",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "prettier": "^1.19.1",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5"
  }
}
